<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.managerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ManagerSide.ReportPage"></activity>
        <activity android:name=".ManagerSide.VerifyPage" />
        <activity android:name=".ManagerSide.SupplierList" />
        <activity android:name=".ManagerSide.HomePage" />
        <activity android:name=".ManagerSide.SignUpPage" />

        <service
            android:name=".SupplierSide.Service.ComingOrder"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ManagerSide.NewSupplier" />
        <activity android:name=".ManagerSide.LoginPage" />
        <activity android:name=".SupplierSide.Login.LoginPage" />
        <activity android:name=".SupplierSide.EditFood.EditFoodActivity" />
        <activity
            android:name=".SupplierSide.HomePage"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>